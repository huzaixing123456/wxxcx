  border-bottom:1rpx solid #EFEFEF;
}

.item5 .t1{
  padding-top:20rpx;
}

.item5 .t3{
  margin-top:20rpx;
  display:flex;
  justify-content:space-between;
}

.item6{
  margin:0 20rpx;
  font-size:26rpx;
  display:flex;
  padding:30rpx 0;
  justify-content:space-between;
}

.item7 .t1{
  color:#000;
  padding:30rpx 0 20rpx;
}

.item7{
  margin:0 20rpx;
  background:#EFEFEF;
  padding:10rpx 30rpx;
  font-size:26rpx;
  color:#666;
}

.btn-wrap {
    display: flex;
    padding: 40rpx;
    background-color: #fff;
    justify-content: flex-end;
}

.btn-wrap .btn{
    text-align: center;
    font-size: 28rpx;
    padding: 10rpx 20rpx;
    height: 35rpx;
    border-radius: 20rpx;
    line-height: 35rpx;
    margin-left: 20rpx;
}

.btn-wrap .btn.bt1 {
    border: 1px solid #FD8238;
}

.btn-wrap .btn.bt2 {
    background-color: #FD8238;
    color: #fff;
}

.dialog{
  position:absolute;
  top:0;
  left:0;
  bottom:0;
  right:0;
  background:rgba(0,0,0,0.5);
}