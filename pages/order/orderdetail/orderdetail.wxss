  margin-top:30rpx;
  padding:30rpx 0;
  display:flex;
  justify-content:space-between;
  align-items:center;
  border-top:1rpx solid #EFEFEF;
  border-bottom:1rpx solid #EFEFEF;
}

.item4 .t2{
  font-size:30rpx;
}

.item4 .t3{
  margin-right:100rpx;
  display:flex;
  align-items:center;
  flex-direction:column;
  color:#FD8238;
  font-size:26rpx;
}

.item .t4{
