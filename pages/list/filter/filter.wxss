}

.wrap2 .items {
    display: -webkit-flex;
    flex-direction: row;
    flex-wrap: wrap;
    margin-top: 20rpx;
    overflow: hidden;
    animation-iteration-count: infinite;
    animation-direction: alternate;
    height: 152rpx;
}

.wrap2 .items .item {
    width: 22%;
    border-radius: 20rpx;
