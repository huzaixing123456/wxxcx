.wrap1 {
    padding: 40rpx;
    line-height: 100rpx;
    font-size:32rpx;
    background: -mos-linear-gradient(#fff,#fcfafa);
    background: linear-gradient(#fff,#fcfafa);
}


.wrap1 .items {
    display: -webkit-flex;
    flex-direction: row;
    flex-wrap: wrap;
}

.wrap1 .item {
    margin-right: 50rpx;
    text-align: center;
}

.wrap1 .item .img{
    background-color: #fff;
    width: 110rpx;
    height: 110rpx;
    border-radius: 50%;
    text-align: center;
    line-height: 110rpx;
    box-shadow: 0 0 8rpx #dbdada;
}

.wrap1 .item .img .t1{
    font-size: 40rpx;
}

.active {
    color: rgb(255,128,16);
}

.wrap2 {
