.count-wrap{
    display:flex;
    flex-direction:row;
}

.count-item{
    flex:1;

}
.count-item .items{
    margin-right:50rpx;
    display:flex;
    justify-content:space-between;
    align-items:center;
    background:#EEE;
    padding:5rpx;
    border-radius:50rpx;
}

.count-item .minus,.count-item .add{
    font-size:40rpx;
    width:50rpx;
    height:50rpx;
    border-radius:50%;
    text-align:center;
    line-height:50rpx;
}

.count-item .minus{
    background:#DDD;
}

.count-item .add{
    background:#fd8238;
    color:#FFF;
}

